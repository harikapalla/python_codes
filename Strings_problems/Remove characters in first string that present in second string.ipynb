    "def rem_fs(fs, ss):\n",
    "    l=[]\n",
    "    for i in range(len(fs)):\n",
    "        if fs[i] in ss:\n",
    "            continue\n",
    "        l.append(fs[i])\n",
    "    return(''.join(l))\n",
    "\n",
    "if __name__==\"__main__\":\n",
    "    fstring = \"helloworld\"\n",
    "    sstring = \"lo\"\n",
    "    print(rem_fs(fstring, sstring))"
   ]
  }
